# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=webhook_manager

# TypeORM
# WARNING: 建议生产环境保持 false，使用 migrations 管理 schema
DB_SYNCHRONIZE=true
DB_LOGGING=false
DB_SSL=false

# Connection pool (pg.Pool)
DB_POOL_MAX=10
DB_POOL_IDLE_TIMEOUT_MS=30000
DB_POOL_CONN_TIMEOUT_MS=2000

# Auth
JWT_SECRET=dev_secret
JWT_EXPIRES_IN=1d
BCRYPT_SALT_ROUNDS=10
